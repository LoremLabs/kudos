{
  "name": "setler",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm run tailwind:watch\" \"pnpm run dev:only\" ",
    "dev:only": "vite dev",
    "build": "concurrently \"pnpm run tailwind:build\" \"pnpm run build:only\"",
    "build:only": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "tailwind:watch": "cross-env TAILWIND_MODE=watch cross-env NODE_ENV=development postcss src/styles/tailwind.css -o src/styles/tailwind-output.css -w",
    "tailwind:build": "cross-env TAILWIND_MODE=build cross-env NODE_ENV=production postcss src/styles/tailwind.css -o src/styles/tailwind-output.css",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "tauri": "tauri"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.2.0",
    "knex": "^2.3.0",
    "tauri-plugin-sqlite-api": "github:lzdyes/tauri-plugin-sqlite",
    "tauri-plugin-stronghold-api": "github:tauri-apps/tauri-plugin-stronghold",
    "timeago.js": "^4.0.2"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@noble/hashes": "^1.1.5",
    "@scure/bip32": "^1.1.3",
    "@scure/bip39": "^1.1.0",
    "@sveltejs/adapter-static": "next",
    "@sveltejs/kit": "next",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tauri-apps/cli": "^1.2.3",
    "autoprefixer": "^10.4.13",
    "better-sqlite3": "^8.0.1",
    "bip39": "^3.0.4",
    "buffer": "^6.0.3",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "d3-array": "^3.2.1",
    "d3-sankey": "^0.12.3",
    "d3-scale": "^4.0.2",
    "d3-scale-chromatic": "^3.0.0",
    "d3-shape": "^3.2.0",
    "esbuild": "^0.17.4",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "hash.js": "^1.1.7",
    "micro-ed25519-hdkey": "^0.1.2",
    "nanoid": "^4.0.0",
    "node-stdlib-browser": "^1.2.0",
    "postcss": "^8.4.20",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.1",
    "prettier-plugin-svelte": "^2.9.0",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "pretty-print-json": "^1.3.1",
    "process": "^0.11.10",
    "ripple-address-codec": "^4.2.4",
    "ripple-bip32": "^0.0.3",
    "ripple-keypairs": "^1.1.4",
    "safe-stable-stringify": "^2.4.1",
    "svelte": "^3.54.0",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^5.0.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.8.4",
    "vite": "4.0.4",
    "vite-plugin-top-level-await": "^1.2.2",
    "xrpl": "2.8.0-beta.0"
  }
}
