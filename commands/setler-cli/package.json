{
  "name": "setler-cli",
  "version": "0.0.0",
  "description": "setler command-line tool",
  "license": "MIT",
  "type": "module",
  "bin": {
    "setler": "src/cli.js"
  },
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "pretty": "prettier --check .",
    "format": "prettier --write . && eslint --fix --ignore-path .gitignore .",
    "pretty:fix": "prettier --write .",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "prettier --write . && eslint --fix --ignore-path .gitignore .",
    "release": "np",
    "test": "echo nyet && exit 0"
  },
  "files": [
    "src/cli.js",
    "src"
  ],
  "keywords": [
    "cli",
    "kudos",
    "xrpl"
  ],
  "dependencies": {
    "@dnslink/js": "^0.12.0",
    "@iarna/toml": "^2.2.5",
    "@noble/hashes": "^1.2.0",
    "@scure/bip39": "^1.1.1",
    "bcrypt": "^5.1.0",
    "better-sqlite3": "^7.5.3",
    "chalk": "^4.1.2",
    "conf": "^10.0.3",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "ed25519-keygen": "^0.3.0",
    "ethers": "^5.7.2",
    "get-stdin": "^9.0.0",
    "glob": "^8.0.3",
    "hash.js": "^1.1.7",
    "keytar": "^7.9.0",
    "knex": "^2.3.0",
    "meow": "^9.0.0",
    "mkdirp": "^1.0.4",
    "node-fetch": "^3.1.0",
    "object-sha": "^2.0.7",
    "open": "^8.4.0",
    "parse-author": "^2.0.0",
    "prompts": "^2.4.2",
    "readline": "^1.3.0",
    "ripple-address-codec": "^4.2.5",
    "update-notifier": "^5.1.0",
    "utils": "link:@noble/hashes/utils",
    "uuid": "^8.3.2",
    "xrpl": "^2.7.0",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "env-paths": "^3.0.0",
    "eslint": "8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "np": "^7.6.2",
    "prettier": "2.4.1"
  },
  "main": "src/cli.js",
  "directories": {
    "doc": "docs"
  },
  "author": {
    "name": "matt mankins",
    "email": "mankins@gmail.com"
  }
}
